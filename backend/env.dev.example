# Development Environment Configuration
NODE_ENV=development

# RAG Configuration
RAG_ENABLED=true
DEV_NO_AUTH=true
DEV_USER_ID=dev-cli
DEV_IMPERSONATE=true
DEV_RELAX_MEMBERSHIP=true
DEV_SHADOW_USER_ID=
ALLOW_DEV_SHADOW_HEADER=true

# Embedding Configuration
USE_EMBEDDINGS=true
EMBEDDINGS_PROVIDER=google
GOOGLE_API_KEY=your-google-api-key-here
GOOGLE_EMBED_MODEL=text-embedding-004
JINA_API_KEY=your-jina-api-key-here
JINA_EMBED_MODEL=jina-embeddings-v3
EMBEDDINGS_DIM=1536

# Other RAG Features
USE_RERANK=false
USE_LLAMA_PARSE=false
OCR_PROVIDER=tesseract
OCR_LANGUAGES=eng
OCR_STRATEGY=hi_res
MAX_CONTEXT_CHARS=1200

# Reranker
ENABLE_RERANKER=false
RERANKER_PROVIDER=cohere
RERANKER_TOP_K_INPUT=20
RERANKER_TOP_N=8

# Multi-Query Expansion
ENABLE_MQE=false
MQE_PROVIDER=gemini
MQE_NUM=3
MQE_TEMPERATURE=0.1

LOG_LEGACY_ROUTES=true

# Database (local development)
DATABASE_URL=postgresql://postgres:q7rT5EGCZiOxcE9D@localhost:55432/rag_db

# Typesense (local development)
TYPESENSE_HOST=localhost
TYPESENSE_PORT=8108
TYPESENSE_PROTOCOL=http
TYPESENSE_API_KEY=xyz

# Unstructured (local development)
UNSTRUCTURED_URL=http://localhost:8001

# Optional API Keys (not required for basic dev)
# Note: GOOGLE_API_KEY and JINA_API_KEY are configured above in embedding section
# VOYAGE_API_KEY=your-voyage-key-here
# LLAMA_CLOUD_API_KEY=your-llama-key-here
# COHERE_API_KEY=your-cohere-key-here

# Gradescope session policy
GRADESCOPE_SESSION_MAX_AGE_MIN=120
GRADESCOPE_SESSION_REFRESH_AHEAD_MIN=20
GRADESCOPE_FAILURE_LIMIT=3
GRADESCOPE_RETRY_BASE_MS=400
GRADESCOPE_RETRY_MAX_MS=5000
GRADESCOPE_USER_AGENT="Mozilla/5.0 (Macintosh; Intel Mac OS X) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/126 Safari/537.36"

# Encrypt stored password + cookie jar
ENCRYPTION_KEY=32_characters_random_string_here
