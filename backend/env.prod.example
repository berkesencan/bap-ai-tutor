# Production Environment Configuration
NODE_ENV=production

# RAG Configuration
RAG_ENABLED=true
DEV_NO_AUTH=false
DEV_USER_ID=dev-cli
USE_EMBEDDINGS=true
USE_RERANK=true
USE_LLAMA_PARSE=false
OCR_PROVIDER=google

# Database (Cloud SQL)
DATABASE_URL=postgresql://user:password@/rag_db?host=/cloudsql/project:region:instance

# Typesense (Cloud)
TYPESENSE_HOST=your-typesense-host.com
TYPESENSE_PORT=443
TYPESENSE_PROTOCOL=https
TYPESENSE_API_KEY=your-production-key

# Unstructured (Cloud Run)
UNSTRUCTURED_URL=https://unstructured-service-url.com

# Production API Keys
JINA_API_KEY=your-production-jina-key
VOYAGE_API_KEY=your-production-voyage-key
GOOGLE_DOC_AI_KEY=your-google-doc-ai-key

# Gradescope session policy
GRADESCOPE_SESSION_MAX_AGE_MIN=120
GRADESCOPE_SESSION_REFRESH_AHEAD_MIN=20
GRADESCOPE_FAILURE_LIMIT=3
GRADESCOPE_RETRY_BASE_MS=400
GRADESCOPE_RETRY_MAX_MS=5000
GRADESCOPE_USER_AGENT="Mozilla/5.0 (Macintosh; Intel Mac OS X) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/126 Safari/537.36"

# Encrypt stored password + cookie jar
ENCRYPTION_KEY=32_characters_random_string_here
